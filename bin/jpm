#! /usr/bin/env python

import sys


import jpm


def main(args):
    """Run the program"""
    try:
        for arg in args:
            jpm.play(arg)
    except Exception, e:
        print >> sys.stderr, e
        return not os.EX_OK
    return os.EX_OK


if __name__ == '__main__':
    sys.exit(jpm.main(sys.argv[1:]))
